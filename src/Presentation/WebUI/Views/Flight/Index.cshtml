@*<script>

    function openModal(button) {
    document.getElementById("customModal").style.display = "flex";
    const from = button.getAttribute("data-from");
    const to = button.getAttribute("data-to");
    const date = button.getAttribute("data-date");
    const toTime=button.getAttribute("data-toTime");
    const arrTime=button.getAttribute("data-arrTime");

    const ecoPrice = parseFloat(button.getAttribute("data-ecoprice"));
    const classicPrice = ecoPrice + 70;
    const premiumPrice = ecoPrice + 120;
    document.querySelector(".flightModalSpan").textContent = `${from} - ${to}, ${date}`;
    document.querySelector(".flightDateSpan").textContent=`${toTime} -> ${arrTime}`;

    const tariffCards = document.querySelectorAll(".tariff-card");
    tariffCards[0].querySelector("h4").textContent = "Budget";
    tariffCards[0].querySelector(".flightModalPrice").textContent = `${ecoPrice.toFixed(2)} €`;

   
    tariffCards[1].querySelector("h4").textContent = "Classic";
    tariffCards[1].querySelector(".flightModalPrice").textContent = `${classicPrice.toFixed(2)} €`;

   
    tariffCards[2].querySelector("h4").textContent = "Premium";
    tariffCards[2].querySelector(".flightModalPrice").textContent = `${premiumPrice.toFixed(2)} €`;
}

    function closeModal() {
        document.getElementById("customModal").style.display = "none";
    }

</script>*@
