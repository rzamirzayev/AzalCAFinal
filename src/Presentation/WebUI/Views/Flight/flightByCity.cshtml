@model List<FlightSearchDto>
@{
    ViewData["Title"] = "FlightByCity";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="./assets/js/header.js" defer></script>
    <link rel="stylesheet"
          href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css"
          integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh"
          crossorigin="anonymous" />
    <link rel="stylesheet" href="~/./assets/css/header.css" />
    <link rel="stylesheet" href="~/./assets/css/flight.css" />
    <link rel="stylesheet" href="~/./assets/css/flightId.css" />
    <script src="~/./assets/js/header.js" defer></script>
    <style>
        .ontheair {
            display: flex;
            justify-content: center;
            align-items: center;
            color: #2C8DC7;
            padding: 0px 100px;
            border-left: 2px solid lightgrey;
        }

            .ontheair h1 {
                font-size: 28px;
            }
    </style>
    <title>Document</title>
</head>
<body>


    <header class="d-flex">
        <div class="azal_logo">
            <img src="https://bilet.az/wp-content/uploads/2020/03/azal_logo.png"
                 alt="azal_logo" />
        </div>
        <div class="d-flex header_selects">
            <div class="header_select" style="position: relative">
                <button class="head_button d-flex">Currency</button>
                <div class="currencyDiv header_button_div" style="position: absolute">
                    <ul>
                        <li><a href="#">Azn</a></li>
                        <li><a href="#">Usd</a></li>
                        <li><a href="#">Euro</a></li>
                    </ul>
                </div>
            </div>
            <div class="header_select" style="position: relative">
                <button class="head_button d-flex">
                    <svg width="25px"
                         height="25px"
                         viewBox="-2.4 -2.4 28.80 28.80"
                         fill="none"
                         xmlns="http://www.w3.org/2000/svg"
                         stroke="#000"
                         transform="rotate(0)">
                        <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                        <g id="SVGRepo_tracerCarrier"
                           stroke-linecap="round"
                           stroke-linejoin="round"></g>
                        <g id="SVGRepo_iconCarrier">
                            <circle cx="12"
                                    cy="12"
                                    r="8"
                                    stroke="#fff"
                                    stroke-width="2"></circle>
                            <path d="M18.2075 6.78485C17.4747 7.46088 16.5544 8.00912 15.5106 8.39144C14.4668 8.77376 13.3244 8.981 12.163 8.99875C11.0016 9.01651 9.84894 8.84434 8.7852 8.49423C7.72146 8.14412 6.77215 7.62447 6.00325 6.97139"
                                  stroke="#fff"
                                  stroke-width="2"></path>
                            <path d="M18.3222 17.3235C17.5826 16.608 16.6367 16.0274 15.5553 15.6251C14.474 15.2227 13.2853 15.0091 12.0783 15.0003C10.8713 14.9914 9.67723 15.1875 8.58561 15.5739C7.494 15.9603 6.53311 16.5269 5.77495 17.2314"
                                  stroke="#fff"
                                  stroke-width="2"></path>
                            <path d="M12 4V20" stroke="#fff" stroke-width="2"></path>
                            <path d="M20 12H4" stroke="#fff" stroke-width="2"></path>
                        </g>
                    </svg>
                    English
                </button>
                <div class="languageDiv header_button_div" style="position: absolute">
                    <ul>
                        <li><a href="#">Azerbaycan</a></li>
                        <li><a href="#">Ingilisce</a></li>
                        <li><a href="#">Rusca</a></li>
                    </ul>
                </div>
            </div>

            <div class="header_select" style="position: relative">
                <button class="head_button d-flex">
                    <svg width="25px"
                         height="25px"
                         viewBox="0 0 512.00 512.00"
                         version="1.1"
                         xml:space="preserve"
                         xmlns="http://www.w3.org/2000/svg"
                         xmlns:xlink="http://www.w3.org/1999/xlink"
                         fill="#fffffff"
                         stroke="#fffffff"
                         stroke-width="12.8"
                         transform="rotate(0)">
                    <g id="SVGRepo_bgCarrier" stroke-width="0"></g>
                    <g id="SVGRepo_tracerCarrier"
                       stroke-linecap="round"
                       stroke-linejoin="round"
                       stroke="#CCCCCC"
                       stroke-width="9.216"></g>
                    <g id="SVGRepo_iconCarrier">
                    <g id="Layer_1"></g>
                    <g id="Layer_2">
                    <g>
                    <path class="st0"
                          d="M256,240.9c27.17,0,53.02-13.76,72.79-38.74c17.74-22.42,27.92-50.59,27.92-77.28 c0-27.1-10.74-52.05-30.25-70.28C307.74,37.13,282.72,27.5,256,27.5s-51.74,9.63-70.45,27.1c-19.51,18.23-30.25,43.18-30.25,70.28 c0,26.7,10.17,54.87,27.92,77.28C202.98,227.15,228.83,240.9,256,240.9z M207.4,77.99C220.16,66.07,237.42,59.5,256,59.5 s35.84,6.57,48.6,18.49c12.96,12.11,20.1,28.76,20.1,46.9c0,38.06-30.65,84.02-68.7,84.02s-68.7-45.96-68.7-84.02 C187.3,106.75,194.44,90.09,207.4,77.99z"></path>
                    <path class="st0"
                          d="M103.34,311.64c-10.38,20.53-13.71,45.13-10.19,75.22c3.86,32.95,8.97,61.01,15.61,85.78 c1.87,6.99,8.21,11.86,15.45,11.86H387.8c7.24,0,13.58-4.87,15.46-11.86c6.64-24.81,11.74-52.87,15.58-85.77 c3.53-30.08,0.2-54.68-10.18-75.22c-10.61-21.01-29.95-39.66-54.45-52.53c-5.38-2.82-11.04-5.4-16.81-7.65 c-4.63-1.81-9.84-1.36-14.09,1.2C302.47,265.25,279.2,271.9,256,271.9c-23.19,0-46.47-6.65-67.31-19.23 c-4.25-2.57-9.46-3.01-14.09-1.2c-5.77,2.26-11.43,4.83-16.81,7.66C133.28,271.99,113.95,290.64,103.34,311.64z M172.65,287.46 c2.18-1.14,4.42-2.24,6.7-3.28c23.96,12.92,50.29,19.72,76.64,19.72c26.35,0,52.68-6.79,76.64-19.72c2.28,1.04,4.52,2.13,6.7,3.27 c18.65,9.79,33.12,23.5,40.76,38.62c7.56,14.96,9.77,33.09,6.96,57.05c0,0,0,0.01,0,0.01c-3.06,26.15-6.91,48.98-11.73,69.36 H136.68c-4.81-20.35-8.68-43.18-11.74-69.36c-2.81-23.98-0.59-42.11,6.97-57.07C139.53,310.97,154.01,297.25,172.65,287.46z"></path>
                  </g>
                </g>
              </g>
            </svg>
                    Personal Account
                </button>
                <div class="accountDiv header_button_div" style="position: absolute">
                    <ul>
                        <li><a href="#">Register</a></li>
                        <li><a href="#">Login</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </header>
    <main>
        <div class="d-flex flightIdHead" style="flex-direction: column">
            <h1>Flight Status</h1>
        </div>

        <div class="finalFlightDate">
            @foreach(var flight in Model)
            {
                <div class="flight_date d-flex">
                    <div class="left_column">
                        <div class="d-flex">
                            <svg xmlns="http://www.w3.org/2000/svg"
                             width="20"
                             height="20"
                             fill="none"
                             viewBox="0 0 20 20"
                             class="root_ESF size_20__Il">
                                <g id="logo">
                                    <g id="Group 135">
                                        <path id="path23491"
                                          fill="#036"
                                          d="M15.154 12.916c-.868 2.63-3.347 4.534-6.264 4.534a6.604 6.604 0 0 1-6.596-6.597A6.604 6.604 0 0 1 8.89 4.257c1.108 0 2.152.275 3.069.76l.236-.208a6.847 6.847 0 0 0-3.305-.846c-3.8 0-6.89 3.09-6.89 6.89s3.09 6.89 6.89 6.89c3.086 0 5.705-2.04 6.58-4.842l-.316.015Z"></path>
                                        <path id="path23493"
                                          fill="#00B2E2"
                                          d="M4.476 12.976c3.978-.366 7.919-.976 11.804-1.955l-.18.806c-4.97 1.133-8.437 1.237-11.624 1.15Z"></path>
                                        <path id="path23495"
                                          fill="#00B2E2"
                                          d="M5.108 12.581a69.207 69.207 0 0 0 11.459-2.838l-.185.825c-4.346 1.366-8.051 1.976-11.274 2.013Z"></path>
                                        <path id="path23497"
                                          fill="#57C8E7"
                                          d="M5.715 12.161c3.654-.911 7.375-2.186 11.141-3.713l-.185.83c-3.99 1.566-7.68 2.597-10.956 2.883Z"></path>
                                        <path id="path23499"
                                          fill="#98DAE9"
                                          d="M6.32 11.664c3.685-1.12 7.309-2.803 10.895-4.821l-.227 1.017c-4.296 2.133-7.936 3.5-10.668 3.804Z"></path>
                                        <path id="path23501"
                                          fill="#57C8E7"
                                          d="M7.104 11.139c4.138-2.001 7.502-4.074 10.533-6.18l-.25 1.121C13.95 8.274 10.52 10.091 7.103 11.14Z"></path>
                                        <path id="path23503"
                                          fill="#00B2E2"
                                          d="M7.707 10.488c4.069-2.571 7.598-5.188 10.451-7.86l-.306 1.369c-3.375 2.913-6.83 4.919-10.145 6.491Z"></path>
                                        <path id="path23505"
                                          fill="#00B2E2"
                                          d="m18.391 1-1.15.03a50.476 50.476 0 0 1-4.302 4.62c-1.563 1.489-3.134 2.824-4.707 4.138C11.677 7.464 15.095 4.861 18.392 1Z"></path>
                                    </g>
                                </g>
                            </svg>
                            Azerbaijan Airlines
                        </div>
                        <div class="d-flex arrivalDate">
                            <div class="dateInfo">
                                <p>@flight.FlightSchedules?[0].DepartureTime.ToString().Substring(0,5)</p>
                                <div class="planeSvg">
                                    <svg xmlns="http://www.w3.org/2000/svg"
                                     width="20"
                                     height="20"
                                     fill="currentColor"
                                     viewBox="0 0 20 20"
                                     class="root_ESF size_20__Il">
                                        <path d="M5.196 2.624a.837.837 0 0 0-.604 1.405l3.501 3.783-3.205 1.401-1.973-1.158a.828.828 0 0 0-1.12 1.157l1.116 1.772a2.355 2.355 0 0 0 2.935.902l11.64-5.095a1.413 1.413 0 0 0-1.131-2.589l-4.377 1.913-6.38-3.394a.836.836 0 0 0-.402-.097Zm-2.071 13.21a.628.628 0 0 0-.634.624.625.625 0 0 0 .634.625h13.75a.624.624 0 1 0 0-1.25H3.125Z"></path>
                                    </svg>
                                </div>
                                <span style="color: #2e3034">@flight.DepartureAirport</span>
                                <span>Terminal 1</span>
                                <span>@flight.FlightDate</span>
                            </div>
                            <div class="svgFlight">
                                <div class="salt"></div>
                                <div class="round"></div>
                                <div class="round"></div>
                            </div>
                            <div class="dateInfo">
                                <p>@flight.FlightSchedules?[0].ArrivalTime.ToString().Substring(0,5)</p>
                                <div class="planeSvg">
                                    <svg xmlns="http://www.w3.org/2000/svg"
                                     width="20"
                                     height="20"
                                     fill="currentColor"
                                     viewBox="0 0 20 20"
                                     class="root_ESF size_20__Il">
                                        <path d="M8.964 1.667a.853.853 0 0 0-.837.865l.078 5.11-3.383-1.119-.708-2.22a.844.844 0 0 0-1.637.113l-.36 2.104A2.4 2.4 0 0 0 3.73 9.202l12.295 4.058a1.44 1.44 0 0 0 .904-2.733L12.25 8.98l-2.47-6.752a.852.852 0 0 0-.815-.56Zm-5.84 14.166a.624.624 0 1 0 0 1.25h13.75a.628.628 0 0 0 .586-.383.626.626 0 0 0-.585-.867H3.125Z"></path>
                                    </svg>
                                </div>
                                <span style="color: #2e3034">@flight.DestinationAirport</span>
                                <span>Terminal 1</span>
                                <span>@flight.FlightDate</span>
                            </div>
                        </div>
                    </div>
                    <div class="right_column d-flex">
                        <div class="flightInfoContainer">
                            <div class="svgContainer d-flex">
                                <div>
                                    <svg xmlns="http://www.w3.org/2000/svg"
                                     fill="none"
                                     viewBox="1.5 5 21 14"
                                     class="root_ESF size_16_MSX">
                                        <path fill="currentColor"
                                          d="M21.8 9.265a.75.75 0 0 0 .7-.75V6.75c0-.965-.785-1.75-1.75-1.75H16.5v1A.749.749 0 1 1 15 6V5H3.25c-.965 0-1.75.785-1.75 1.75v1.765a.75.75 0 0 0 .7.75c1.43.09 2.55 1.295 2.55 2.735s-1.12 2.645-2.55 2.735a.75.75 0 0 0-.7.75v1.765c0 .965.785 1.75 1.75 1.75H15v-1a.749.749 0 1 1 1.5 0v1h4.25c.965 0 1.75-.785 1.75-1.75v-1.765a.75.75 0 0 0-.7-.75c-1.43-.09-2.55-1.295-2.55-2.735s1.12-2.645 2.55-2.735ZM16.5 15a.749.749 0 1 1-1.5 0v-1.5a.749.749 0 1 1 1.5 0V15Zm0-4.5a.749.749 0 1 1-1.5 0V9a.749.749 0 1 1 1.5 0v1.5Z"></path>
                                    </svg>
                                    J2 71
                                </div>
                                <div>
                                    <svg xmlns="http://www.w3.org/2000/svg"
                                     width="16"
                                     height="16"
                                     fill="currentColor"
                                     viewBox="0 0 16 16"
                                     class="root_ESF size_16_MSX">
                                        <path d="M13.333 6.667H10L6.62 1.66a.698.698 0 0 0-1.256.576l1.628 4.43h-3.66l-.92-1.134c-.1-.12-.186-.2-.34-.2h-.406a.33.33 0 0 0-.32.414L2 8l-.653 2.253a.33.33 0 0 0 .32.414h.406c.154 0 .24-.08.34-.2l.92-1.134h3.66l-1.627 4.43a.697.697 0 0 0 1.257.576L10 9.333h3.333a1.333 1.333 0 1 0 0-2.666Z"></path>
                                    </svg>
                                    @flight.Airplane
                                </div>
                            </div>
                            <p>4 h 30 min</p>
                            <svg xmlns="http://www.w3.org/2000/svg"
                             width="20"
                             height="20"
                             fill="currentColor"
                             viewBox="0 0 20 20"
                             class="root_ESF direct_icon_vMk size_20__Il withoutSvg">
                                <path fill="#2C8DC7"
                                  d="M8.535 5.41A.625.625 0 0 0 8.1 6.484l3.1 3.1H1.875a.625.625 0 1 0 0 1.25H11.2l-3.1 3.1a.625.625 0 1 0 .884.883l4.166-4.167a.625.625 0 0 0 0-.884L8.984 5.6a.625.625 0 0 0-.449-.19Zm7.923 2.507a2.294 2.294 0 0 0-2.291 2.291 2.294 2.294 0 0 0 2.291 2.292 2.294 2.294 0 0 0 2.292-2.292 2.294 2.294 0 0 0-2.292-2.291Z"></path>
                            </svg>
                            <br />
                            <span style="margin-top: 8px">Without Stops</span>
                        </div>
                        <div class="ontheair"><h1>@flight.FlightSchedules?[0].onTheAir</h1></div>
                    </div>
                </div>
            }
 
        </div>
    </main>
</body>
</html>
